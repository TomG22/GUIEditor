PROJ_NAME := GUIEditor


###  Windows makefile targets ###

all: $(PROJ_NAME)


configure:
	cmd.exe /C "build.bat configure"


$(PROJ_NAME): configure
	cmd.exe /C "build.bat build"


build: $(PROJ_NAME)


rebuild: clean
	cmd.exe /C "build.bat configure"
	cmd.exe /c "build.bat build"


clean:
	cmd.exe /C "build.bat clean"


.PHONY: all configure $(PROJ_NAME) build rebuild clean
